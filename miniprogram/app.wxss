.body {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 50rpx;
  box-sizing: border-box;
}

@keyframes floatUp{
  from{
    opacity: 0;
    transform: translateY(300px);
  }
  to{
    opacity: 1;
    transform: translateY(0);
  }
}